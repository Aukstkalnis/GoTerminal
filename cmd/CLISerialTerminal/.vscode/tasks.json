{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "go",
			"args": ["build"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher":"$go"
		},
		{
			"label": "Run",
			"type": "shell",
			"command": "${workspaceFolder}\\${workspaceFolderBasename}.exe",
			"presentation": {
				"clear": true
			},
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"problemMatcher":[]
		}
	]
}